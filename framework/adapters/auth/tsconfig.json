{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../../tooling/tsconfig/base.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "lib": [
      "ES2022"
    ],
    "verbatimModuleSyntax": true,
    "composite": false,
    "incremental": false,
    "types": [
      "node",
      "vitest/globals"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".turbo"
  ]
}