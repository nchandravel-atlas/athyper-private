{
  "name": "@athyper/adapter-objectstorage",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "src"],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts src/types.ts src/s3/client.ts src/s3/operations.ts --format esm --sourcemap --outDir dist && tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsup src/index.ts src/types.ts src/s3/client.ts src/s3/operations.ts --format esm --dts --sourcemap --outDir dist --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@athyper/core": "workspace:*",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0"
  },
  "devDependencies": {
    "@athyper/tsconfig": "workspace:*",
    "tsup": "^8.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}
