{
  "$schema": "athyper://dashboard-contribution/v1",
  "module_code": "ASSETREMS",
  "module_name": "Real Estate Asset Management",
  "dashboards": [
    {
      "code": "assetrems_overview",
      "title_key": "dashboard.ASSETREMS.overview.title",
      "description_key": "dashboard.ASSETREMS.overview.description",
      "icon": "home",
      "workbenches": ["user", "admin"],
      "sort_order": 100,
      "acl": [
        { "principal_type": "persona", "principal_key": "agent", "permission": "view" },
        { "principal_type": "persona", "principal_key": "manager", "permission": "view" },
        { "principal_type": "persona", "principal_key": "module_admin", "permission": "edit" },
        { "principal_type": "persona", "principal_key": "tenant_admin", "permission": "edit" }
      ],
      "layout": {
        "schema_version": 1,
        "columns": 12,
        "row_height": 80,
        "items": [
          { "id": "h1", "widget_type": "heading", "params": { "text_key": "dashboard.ASSETREMS.overview.title", "level": "h2" }, "grid": { "x": 0, "y": 0, "w": 12, "h": 1 } },
          { "id": "kpi1", "widget_type": "kpi", "params": { "label_key": "dashboard.ASSETREMS.overview.kpi.total_properties", "query_key": "assetrems.total_properties", "format": "number" }, "grid": { "x": 0, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi2", "widget_type": "kpi", "params": { "label_key": "dashboard.ASSETREMS.overview.kpi.occupancy_rate", "query_key": "assetrems.occupancy_rate", "format": "percent" }, "grid": { "x": 3, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi3", "widget_type": "kpi", "params": { "label_key": "dashboard.ASSETREMS.overview.kpi.rent_due", "query_key": "assetrems.rent_due", "format": "currency" }, "grid": { "x": 6, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi4", "widget_type": "kpi", "params": { "label_key": "dashboard.ASSETREMS.overview.kpi.lease_expiring", "query_key": "assetrems.lease_expiring", "format": "number" }, "grid": { "x": 9, "y": 1, "w": 3, "h": 2 } },
          { "id": "chart1", "widget_type": "chart", "params": { "title_key": "dashboard.ASSETREMS.overview.chart.occupancy_trend", "query_key": "assetrems.occupancy_trend", "chart_type": "line" }, "grid": { "x": 0, "y": 3, "w": 8, "h": 4 } },
          { "id": "list1", "widget_type": "list", "params": { "title_key": "dashboard.ASSETREMS.overview.list.recent_leases", "query_key": "assetrems.recent_leases", "columns": ["date", "property", "tenant", "rent"], "page_size": 5 }, "grid": { "x": 8, "y": 3, "w": 4, "h": 4 } }
        ]
      }
    }
  ]
}
