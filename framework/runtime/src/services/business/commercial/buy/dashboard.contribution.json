{
  "$schema": "athyper://dashboard-contribution/v1",
  "module_code": "BUY",
  "module_name": "Buying",
  "dashboards": [
    {
      "code": "buy_overview",
      "title_key": "dashboard.BUY.overview.title",
      "description_key": "dashboard.BUY.overview.description",
      "icon": "shopping-cart",
      "workbenches": ["user", "admin"],
      "sort_order": 100,
      "acl": [
        { "principal_type": "persona", "principal_key": "agent", "permission": "view" },
        { "principal_type": "persona", "principal_key": "manager", "permission": "view" },
        { "principal_type": "persona", "principal_key": "module_admin", "permission": "edit" },
        { "principal_type": "persona", "principal_key": "tenant_admin", "permission": "edit" }
      ],
      "layout": {
        "schema_version": 1,
        "columns": 12,
        "row_height": 80,
        "items": [
          { "id": "h1", "widget_type": "heading", "params": { "text_key": "dashboard.BUY.overview.title", "level": "h2" }, "grid": { "x": 0, "y": 0, "w": 12, "h": 1 } },
          { "id": "kpi1", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.overview.kpi.open_pos", "query_key": "buy.open_pos", "format": "number" }, "grid": { "x": 0, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi2", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.overview.kpi.pending_receipts", "query_key": "buy.pending_receipts", "format": "number" }, "grid": { "x": 3, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi3", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.overview.kpi.spend_mtd", "query_key": "buy.spend_mtd", "format": "currency" }, "grid": { "x": 6, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi4", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.overview.kpi.overdue_orders", "query_key": "buy.overdue_orders", "format": "number" }, "grid": { "x": 9, "y": 1, "w": 3, "h": 2 } },
          { "id": "chart1", "widget_type": "chart", "params": { "title_key": "dashboard.BUY.overview.chart.spend_by_category", "query_key": "buy.spend_by_category", "chart_type": "pie" }, "grid": { "x": 0, "y": 3, "w": 8, "h": 4 } },
          { "id": "list1", "widget_type": "list", "params": { "title_key": "dashboard.BUY.overview.list.recent_pos", "query_key": "buy.recent_pos", "columns": ["date", "supplier", "reference", "amount"], "page_size": 5 }, "grid": { "x": 8, "y": 3, "w": 4, "h": 4 } }
        ]
      }
    },
    {
      "code": "buy_partner",
      "title_key": "dashboard.BUY.partner.title",
      "description_key": "dashboard.BUY.partner.description",
      "icon": "shopping-cart",
      "workbenches": ["partner"],
      "sort_order": 100,
      "acl": [
        { "principal_type": "persona", "principal_key": "partner", "permission": "view" }
      ],
      "layout": {
        "schema_version": 1,
        "columns": 12,
        "row_height": 80,
        "items": [
          { "id": "h1", "widget_type": "heading", "params": { "text_key": "dashboard.BUY.partner.title", "level": "h2" }, "grid": { "x": 0, "y": 0, "w": 12, "h": 1 } },
          { "id": "kpi1", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.partner.kpi.my_pos", "query_key": "buy.partner.my_pos", "format": "number" }, "grid": { "x": 0, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi2", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.partner.kpi.pending_deliveries", "query_key": "buy.partner.pending_deliveries", "format": "number" }, "grid": { "x": 3, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi3", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.partner.kpi.invoices", "query_key": "buy.partner.invoices", "format": "number" }, "grid": { "x": 6, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi4", "widget_type": "kpi", "params": { "label_key": "dashboard.BUY.partner.kpi.payments", "query_key": "buy.partner.payments", "format": "number" }, "grid": { "x": 9, "y": 1, "w": 3, "h": 2 } },
          { "id": "chart1", "widget_type": "chart", "params": { "title_key": "dashboard.BUY.partner.chart.delivery_schedule", "query_key": "buy.partner.delivery_schedule", "chart_type": "bar" }, "grid": { "x": 0, "y": 3, "w": 8, "h": 4 } },
          { "id": "list1", "widget_type": "list", "params": { "title_key": "dashboard.BUY.partner.list.my_orders", "query_key": "buy.partner.my_orders", "columns": ["date", "reference", "status", "amount"], "page_size": 5 }, "grid": { "x": 8, "y": 3, "w": 4, "h": 4 } }
        ]
      }
    }
  ]
}
