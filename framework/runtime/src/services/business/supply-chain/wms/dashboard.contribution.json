{
  "$schema": "athyper://dashboard-contribution/v1",
  "module_code": "WMS",
  "module_name": "Warehouse Management",
  "dashboards": [
    {
      "code": "wms_overview",
      "title_key": "dashboard.WMS.overview.title",
      "description_key": "dashboard.WMS.overview.description",
      "icon": "warehouse",
      "workbenches": ["user", "admin"],
      "sort_order": 100,
      "acl": [
        { "principal_type": "persona", "principal_key": "agent", "permission": "view" },
        { "principal_type": "persona", "principal_key": "manager", "permission": "view" },
        { "principal_type": "persona", "principal_key": "module_admin", "permission": "edit" },
        { "principal_type": "persona", "principal_key": "tenant_admin", "permission": "edit" }
      ],
      "layout": {
        "schema_version": 1,
        "columns": 12,
        "row_height": 80,
        "items": [
          { "id": "h1", "widget_type": "heading", "params": { "text_key": "dashboard.WMS.overview.title", "level": "h2" }, "grid": { "x": 0, "y": 0, "w": 12, "h": 1 } },
          { "id": "kpi1", "widget_type": "kpi", "params": { "label_key": "dashboard.WMS.overview.kpi.putaway_pending", "query_key": "wms.putaway_pending", "format": "number" }, "grid": { "x": 0, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi2", "widget_type": "kpi", "params": { "label_key": "dashboard.WMS.overview.kpi.pick_orders", "query_key": "wms.pick_orders", "format": "number" }, "grid": { "x": 3, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi3", "widget_type": "kpi", "params": { "label_key": "dashboard.WMS.overview.kpi.packing", "query_key": "wms.packing", "format": "number" }, "grid": { "x": 6, "y": 1, "w": 3, "h": 2 } },
          { "id": "kpi4", "widget_type": "kpi", "params": { "label_key": "dashboard.WMS.overview.kpi.cycle_count_due", "query_key": "wms.cycle_count_due", "format": "number" }, "grid": { "x": 9, "y": 1, "w": 3, "h": 2 } },
          { "id": "chart1", "widget_type": "chart", "params": { "title_key": "dashboard.WMS.overview.chart.throughput", "query_key": "wms.throughput", "chart_type": "bar" }, "grid": { "x": 0, "y": 3, "w": 8, "h": 4 } },
          { "id": "list1", "widget_type": "list", "params": { "title_key": "dashboard.WMS.overview.list.recent_operations", "query_key": "wms.recent_operations", "columns": ["date", "type", "zone", "quantity"], "page_size": 5 }, "grid": { "x": 8, "y": 3, "w": 4, "h": 4 } }
        ]
      }
    }
  ]
}
