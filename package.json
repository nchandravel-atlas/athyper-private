{
  "name": "athyper",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20.11.0 <21"
  },
  "scripts": {
    "dev": "turbo run dev --parallel --continue",
    "build": "turbo run build",
    "build:all": "turbo run build --continue",
    "lint": "turbo run lint",
    "lint:all": "turbo run lint --continue",
    "lint:root": "eslint . --cache --max-warnings=0",
    "lint:fix": "eslint . --fix --cache",
    "typecheck": "turbo run typecheck",
    "typecheck:all": "turbo run typecheck --continue",
    "test": "turbo run test",
    "test:all": "turbo run test --continue",
    "test:policy": "pnpm --filter @athyper/runtime test:policy",
    "test:policy:ci": "pnpm --filter @athyper/runtime test:policy:ci",
    "clean": "turbo run clean && rimraf .turbo",
    "clean:deps": "rimraf node_modules .turbo",
    "clean:all": "pnpm run clean && pnpm run clean:deps && rimraf pnpm-lock.yaml",
    "clean:reset": "pnpm run clean:all && pnpm install && pnpm run build",
    "clean:deep": "pnpm run clean:all && node tooling/devtools/clean-deep.mjs",
    "format:changed": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,yaml,yml}\"",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "depcheck": "depcruise --config .dependency-cruiser.cjs framework packages products tooling tools --output-type err",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run depcheck",
    "check:ci": "pnpm run format:check && pnpm run check",
    "deps:why": "pnpm -r why",
    "deps:outdated": "pnpm -r outdated",
    "doctor": "node -e \"console.log('node', process.version)\" && pnpm -v && turbo --version",
    "prune:neon-web": "turbo prune --scope=@neon/web --docker",
    "mesh:up": "docker compose -f mesh/compose/compose.yml --env-file mesh/env/.env up -d",
    "mesh:down": "docker compose -f mesh/compose/compose.yml --env-file mesh/env/.env down",
    "mesh:logs": "docker compose -f mesh/compose/compose.yml --env-file mesh/env/.env logs -f --tail=200",
    "mesh:ps": "docker compose -f mesh/compose/compose.yml --env-file mesh/env/.env ps",
    "db:provision": "pnpm -F @athyper/adapter-db db:provision",
    "db:provision:ddl": "pnpm -F @athyper/adapter-db db:provision:ddl",
    "db:provision:seed": "pnpm -F @athyper/adapter-db db:provision:seed",
    "db:provision:reset": "pnpm -F @athyper/adapter-db db:provision:reset",
    "db:provision:status": "pnpm -F @athyper/adapter-db db:provision:status",
    "db:provision:force": "pnpm -F @athyper/adapter-db db:provision:force",
    "db:migrate": "pnpm -F @athyper/adapter-db prisma:migrate",
    "db:deploy": "pnpm -F @athyper/adapter-db prisma:deploy",
    "db:reset": "pnpm -F @athyper/adapter-db prisma migrate reset --schema prisma/schema.prisma",
    "db:generate": "pnpm -F @athyper/adapter-db prisma:generate",
    "db:studio": "pnpm -F @athyper/adapter-db prisma:studio",
    "kysely:codegen": "pnpm -F @athyper/adapter-db kysely:codegen",
    "db:migrate:dev": "pnpm run db:migrate",
    "db:migrate:deploy": "pnpm run db:deploy",
    "meta:migrate:plan": "pnpm --filter @athyper/runtime migrate:plan",
    "meta:migrate:dev": "pnpm --filter @athyper/runtime migrate:dev",
    "meta:migrate:deploy": "pnpm --filter @athyper/runtime migrate:deploy",
    "meta:migrate:sql": "pnpm --filter @athyper/runtime migrate:sql",
    "athyper:codegen": "pnpm -C tools/codegen run codegen",
    "athyper:codegen:watch": "pnpm -C tools/codegen run codegen:watch",
    "db:publish": "pnpm -C tools/codegen run publish",
    "db:publish:dry-run": "pnpm -C tools/codegen run publish:dry-run",
    "runtime:start": "pnpm --filter @athyper/runtime start",
    "runtime:start:dev": "pnpm --filter @athyper/runtime start:dev",
    "runtime:start:watch": "pnpm --filter @athyper/runtime start:watch"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "prisma-zod-generator",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "prisma": "6.19.2",
      "@prisma/client": "6.19.2"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "dependency-cruiser": "^17.3.7",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-turbo": "^2.8.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rimraf": "^6.1.2",
    "turbo": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
