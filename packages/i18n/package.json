{
  "name": "@athyper/i18n",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./lang/*": "./lang/*",
    "./package.json": "./package.json"
  },
  "files": ["dist", "src", "lang"],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format esm --dts --sourcemap --outDir dist",
    "dev": "tsup src/index.ts --format esm --dts --sourcemap --outDir dist --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@formatjs/intl": "^2.10.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  }
}
